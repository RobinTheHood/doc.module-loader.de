{
	"check_status": true,
	"commit_type": {
		"enable": true,
		"initial_value": "docs",
		"infer_type_from_branch": true,
		"options": [
			{
				"value": "feat",
				"label": "feat"
			},
			{
				"value": "fix",
				"label": "fix"
			},
			{
				"value": "style",
				"label": "style"
			},
			{
				"value": "docs",
				"label": "docs"
			},
			{
				"value": "refactor",
				"label": "refactor"
			},
			{
				"value": "chore",
				"label": "chore"
			},
			{
				"value": "",
				"label": "none"
			}
		]
	},
	"commit_scope": {
		"enable": true,
		"initial_value": "",
		"options": [
			{
				"value": "navigation",
				"label": "navigation"
			},
			{
				"value": "",
				"label": "none"
			}
		]
	},
	"check_ticket": {
		"infer_ticket": true,
		"confirm_ticket": true,
		"add_to_title": true,
		"append_hashtag": false
	},
	"commit_title": {
		"max_size": 70
	},
	"commit_body": {
		"enable": true,
		"required": false
	},
	"commit_footer": {
		"enable": true,
		"initial_value": [],
		"options": [
			"closes",
			"breaking-change",
			"deprecated"
		]
	},
	"breaking_change": {
		"add_exclamation_to_title": true
	},
	"confirm_commit": true
}
